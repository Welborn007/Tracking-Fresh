            android:name="com.trackingfresh.customer.YourOrders.OrderListActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/last_order"/>

        <service android:name="com.trackingfresh.customer.Map.LocationServiceNew" />

        <activity
            android:name="com.trackingfresh.customer.MyProfile.ProfileActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/profile"/>
        <activity
            android:name="com.trackingfresh.customer.OrderTracking.OrderBikerTrackingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>

        <activity
            android:name="com.trackingfresh.customer.DeliveryAddress.UpdateDeleteDeliveryAddress.FetchedDeliveryAddressActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.trackingfresh.customer.ConfirmOrder.ConfirmOrderActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/ordering"/>
        <activity
            android:name="com.trackingfresh.customer.CheckNearestVehicleAvailability.CheckVehicleActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen" />
        <activity
            android:name="com.trackingfresh.customer.VehicleRoute.RouteActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen" />
        <activity
            android:name="com.trackingfresh.customer.TKCash.TKWalletActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/tkfwallet"/>
        <activity
