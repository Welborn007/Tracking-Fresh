            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="Invite Friends"
            />
        <activity
            android:name="com.trackingfresh.customer.HelpAndFAQ.HelpActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/help"/>
        <activity
            android:name="com.trackingfresh.customer.Legal.LegalActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/legal"/>
        <activity
            android:name="com.trackingfresh.customer.DeliveryAddress.UpdateDeliveryAddress.UpdateDeliveryAddressActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/update_address"/>
        <activity
            android:name="com.trackingfresh.customer.ChangePassword.ChangePasswordActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/change_pass"/>
        <activity
            android:name="com.trackingfresh.customer.Settings.SettingsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/settings"/>
        <activity
            android:name="com.trackingfresh.customer.MyOffers.MyOffersActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/offers"/>

        <activity android:name="com.trackingfresh.customer.NotificationList.NotificationListActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/notification"></activity>
        <activity android:name="com.trackingfresh.customer.orderPlace.OrderPlaceActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/order_placed"/>

        <receiver android:name="com.trackingfresh.customer.Map.RestartServiceReceiver" >
        </receiver>
    </application>

</manifest>