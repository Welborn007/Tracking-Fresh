            android:name="com.trackingfresh.customer.DeliveryAddress.DefaultDeliveryAddress.Default_DeliveryAddress"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/delivery_address"/>
        <activity
            android:name="com.trackingfresh.customer.Payment.PaymentDetails"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/payment_details"/>
        <activity
            android:name="com.trackingfresh.customer.Order.OrderReview"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/order_review"/>
        <activity
            android:name="com.trackingfresh.customer.Register.RegisterActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.trackingfresh.customer.OTP.OTP"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen" />

        <service android:name="com.trackingfresh.customer.SNS.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.trackingfresh.customer.SNS.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="com.trackingfresh.customer.ForgetPassword.ForgotPassword_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/forgot_password"/>
